def distance(a, b, c, d):
    dist = ((a - c)**2 + (b - d)**2)**(.5)
    return dist
x1 = int(input())
y1 = int(input())
x2 = int(input())
y2 = int(input())
x3 = int(input())
y3 = int(input())
perimetr = (distance(x1, y1, x2, y2) + 
distance(x1, y1, x3, y3) + 
distance(x2, y2, x3, y3))
print(perimetr)
